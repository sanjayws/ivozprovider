{
    "name": "irontec/ivozprovider/recordings",
    "description": "Multitenant solution for VoIP telephony providers (Recordings module)",
    "authors": [
        {
            "name": "Ivan Alonso",
            "email": "kaian@irontec.com"
        },
        {
            "name": "Mikel Madariaga",
            "email": "mikel@irontec.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../../library/vendor/*/*",
            "options": {
                "symlink": true
            }
        },
        {
            "packagist.org": false
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "IvozProvider\\": "../../library/IvozProvider"
        },
        "psr-4": {
            "": "src/",
            "Ivoz\\": "../../library/Ivoz"
        },
        "classmap": [
            "app/MicroKernel.php"
        ]
    },
    "autoload-dev": {
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "config": {
        "sort-packages": true,
        "preferred-install": {
            "*": "git"
        },
        "platform": {
            "php": "7.0.19"
        }
    },
    "require": {
        "php": ">=7.0.19",
        "debach/zend-mp3": "1.8.*",
        "irontec/ivoz-core-bundle": "^2.0",
        "irontec/ivoz-provider-bundle": "^2.0",
        "symfony/symfony": "3.4.*"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    }
}
